function modalOverlay(open) {
    if (open === true) {
        const overlay = document.querySelector('.modal-overlay');

        if (!overlay) {
            $('body').append('<div class="modal-overlay"></div>');
        }

        $('body').addClass('overflow-hidden');
        $('.modal-overlay').css('height', $(document).height());
    } else {
