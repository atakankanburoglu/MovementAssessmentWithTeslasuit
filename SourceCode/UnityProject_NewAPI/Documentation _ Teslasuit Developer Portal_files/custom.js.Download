// Defining Range of Sizes for Height (top)
function isSizeInTopHeightRange(sexKey, heightKey, subjects) {
    let range = [];

    if (isMale(sexKey)) {
        switch (heightKey) {
            case 'D':
                range = ['XS'];
                break;
            case 'E':
                range = ['XS', 'S'];
                break;
            case 'F':
                range = ['XS', 'S', 'M', 'L', 'XL', 'XXL'];
                break;
            case 'G':
                range = ['S', 'M', 'L', 'XL', 'XXL'];
                break;
            case 'H':
                range = ['M', 'L', 'XL', 'XXL'];
                break;
            case 'J':
                range = ['M', 'L', 'XL', 'XXL'];
                break;
            default:
                break;
        }
    }

    if (isFemale(sexKey)) {
        switch (heightKey) {
            case 'C':
                range = ['XS', 'S', 'M', 'L', 'XL'];
                break;
            case 'D':
            case 'E':
            case 'F':
                range = ['XS', 'S', 'M', 'L', 'XL', 'XXL'];
                break;
            default:
                break;
        }
    }

    if (
        subjects.every((element) => range.includes(element))
    ) {
        return {
            'success': true,
            'range': range
        }
    }

    return {
        'success': false
    };
}

